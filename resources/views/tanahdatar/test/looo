<!-- Footer start -->


<script>
    $(document).ready(function() {
        // Initialize all popovers
        $('[data-toggle="popover"]').popover();

        // Close any open popover when clicking outside
        $(document).on('click', function (e) {
            // Check if the click was outside of any popover
            if (!$(e.target).closest('[data-toggle="popover"]').length) {
                // Close all popovers
                $('[data-toggle="popover"]').popover('hide');
            }
        });

        // Optionally, you can make sure the popover doesn't close when clicked inside
        $('[data-toggle="popover"]').on('click', function (e) {
            e.stopPropagation(); // Prevent the click inside popover from closing it
        });
    });
</script>


<script>
    document.addEventListener('DOMContentLoaded', function () {
        $('.bs-example-modal-lg').modal('show');
    });
</script>
