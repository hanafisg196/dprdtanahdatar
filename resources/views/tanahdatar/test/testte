        <script>
            $(document).ready(function(){
                // Initialize popover
                $('[data-toggle="popover"]').popover({
                    html: true,  // Enable HTML content in popover
                    trigger: 'focus' // To allow focus event
                });

                // Close popover when 'Close' button is clicked
                $(document).on('click', '.close-popover', function() {
                    $('[data-toggle="popover"]').popover('hide');
                });

                // Close any open popover and then open the new one
                $('[data-toggle="popover"]').on('click', function (e) {
                    // Hide any open popover before showing the clicked one
                    $('[data-toggle="popover"]').not(this).popover('hide');
                });

                // Close popover when clicking outside of it
                $(document).on('click', function (e) {
                    if (!$(e.target).closest('[data-toggle="popover"]').length) {
                        $('[data-toggle="popover"]').popover('hide');
                    }
                });
            });
        </script>

        <script>
            document.getElementById('scrollButton').addEventListener('click', function () {
              document.querySelector('#menus').scrollIntoView({
                behavior: 'smooth'
              });
            });
          </script>

        <script>
            document.addEventListener('DOMContentLoaded', function () {
                $('.bs-example-modal-lg').modal('show');
            });
        </script>
